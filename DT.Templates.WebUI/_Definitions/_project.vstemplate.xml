<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>DriveTime WebUI Template</Name>
    <Description>WebUI starter project, includes:
    - a base application with TypeScript and Knockout
    - PushState navigation with Hasher and Crossroads
    - client side builds with Gulp
    - package management with NPM and Bower
    - unit tests with Jasmine and Karma
    - UI framework with Bootstrap</Description>
    <DefaultName>Application.WebUI</DefaultName>
    
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType></ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>    
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>sw-file-icon.png</Icon>
    
    <!-- Indicates how many parent folders this item template should appear in -->
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="DT.Templates.WebUI.csproj" File="DT.Templates.WebUI.csproj" ReplaceParameters="true">      
    </Project>
  </TemplateContent>
</VSTemplate>